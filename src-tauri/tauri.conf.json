{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Student Essay Evaluation System",
  "version": "1.0.0",
  "identifier": "com.unit9assignment.dev",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5174",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Gay",
        "width": 1300,
        "height": 930,
        "resizable": true,
        "fullscreen": false,
        "hiddenTitle": true,
        "maximizable": false,
        "minimizable": false,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
